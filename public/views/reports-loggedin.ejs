<%- include('includes/header') %>
<!-- ERRORS -->
<% adErrors.forEach(function(message) { %>
  <div class="alert alert-danger text-center small py-1"><%= message %></div>
<% }) %>
<!-- GRAPH -->
<hr>
<div class="jumbotron-transparent m-2 mt-4 p-3"><h1 class="display-1 caveat orange-text text-center">Salutations, <%= fName %> and <%= parentName %></h1></div>
<hr>
<canvas id="pieceGraph"></canvas>

<script>
let ctx = document.getElementById('pieceGraph').getContext('2d');
Chart.defaults.global.defaultFontColor = 'white';
let stackedBar = new Chart(ctx, {
  type: 'bar',
  data: {
    datasets: [{
        label: 'Rhythm and Pulse',
        data: [<%- rhythmArray %>],
        backgroundColor: 'rgba(255, 99, 132, 0.9)',
        borderColor: 'rgba(255, 99, 132, 1)',
        borderWidth: 1,
    }, {
        label: 'Coordination and Pitch',
        data: [<%- coordinationArray %>],
        backgroundColor: 'rgba(54, 162, 235, 0.9)',
        borderColor: 'rgba(54, 162, 235, 1)',
        borderWidth: 1,
    }, {
        label: 'Tone and Articulation',
        data: [<%- toneArray %>],
        backgroundColor: 'rgba(255, 206, 86, 0.9)',
        borderColor: 'rgba(255, 206, 86, 1)',
        borderWidth: 1,
    }, {
        label: 'Dynamics and Balance',
        data: [<%- dynamicsArray %>],
        backgroundColor: 'rgba(75, 192, 192, 0.9)',
        borderColor: 'rgba(75, 192, 192, 1)',
        borderWidth: 1,
    }, {
        label: 'Stylistic Awareness',
        data: [<%- stylisticArray %>],
        backgroundColor: 'rgba(153, 102, 255, 0.9)',
        borderColor: 'rgba(153, 102, 255, 1)',
        borderWidth: 1,
    }],
    labels: <%- JSON.stringify(dateLabels) %>
  },
  options: {
    legend: {
      onHover: showExtraComponentInfo
    },
    layout: {
      padding : {
        top: 10,
        bottom: 10
      }
    },
    title: {
      display : true,
      text: ['Track your mastery across the fundamental components of music'],
      fontSize: 20,
      padding: 20
    },
    scales: {
      xAxes: [{
          stacked: true,
          gridLines: {
            display: false ,
          },
      }],
      yAxes: [{
          stacked: true,
          gridLines: {
            display: true ,
            color: "rgba(255, 255, 255, 0.3)"
          },
      }]
    }
  }
});

function showExtraComponentInfo() {
  // add function later as needed
}
</script>
